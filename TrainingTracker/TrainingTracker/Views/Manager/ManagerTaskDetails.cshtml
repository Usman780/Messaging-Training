    function convertDates(dat) {
        datesandTime = dat.split(" ");
        var date = datesandTime[0];
        var time = datesandTime[1];

        var splittedDate = date.split("/");
        var day = splittedDate[1];
        var year = splittedDate[2];
        var month = splittedDate[0];

        var splittedTime = time.split(":");
        var hr = splittedTime[0];
        var min = splittedTime[1];
        var sec = splittedTime[2];

        if (hr == min == sec == 0) {
            min = 1;
        }
        if (month != 0)
            month = month - 1;
        return new Date(year, month, day, hr, min, sec,0);
    }


