    function sortTable(order) {
        var table = $("#example");
        var asc = order === 'asc',

            tbody = table.find('tbody');

        tbody.find('tr').sort(function (a, b) {
            if (asc) {
                $("#asc").hide();
                $("#dsc").show();
                return $('td:first', a).text().localeCompare($('td:first', b).text());
            } else {
