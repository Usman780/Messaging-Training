                            var division = '<option value="' + data[i]['Id'] + '">' + data[i]['Name'] + '</option>';
                            $('#selectAdminDivision').append(division);
                        }
                        //$("#AdminID").val(0);
                    }
                    else if (role == "2" || role == "4") {
                        alert('manager');
                    }
                    else if (role == "3") {
                        alert('employee');
                    }
                    else {
                        alert('No Session!!');
                    }
