<!-- Select Option Modal -->
<div class="modal fade" id="rightOptionModal">

    <div class="modal-dialog">
        <div class="modal-content">

            <!-- Modal Header -->
            <div class="modal-header">
                <h4 class="modal-title text-center">Select Option</h4>
                <button type="button" class="close" data-dismiss="modal">&times;</button>
            </div>

            <!-- Modal body -->
            <div class="modal-body">
                <a onclick="hideRightOptionModal()" class="btn btn-lg btn-primary btn-block" data-toggle="modal" data-target="#updatestatusModal"><span class="btn-label"><i class="glyphicon glyphicon-ok-circle"></i></span> Update Status</a>
